    <schema>
      <key>/schemas/apps/celestia/render/autoMag</key>
      <applyto>/apps/celestia/render/autoMag</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Render body brightness with auto-magnitude.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/cometTails</key>
      <applyto>/apps/celestia/render/cometTails</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Render tails on comet objects.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/markers</key>
      <applyto>/apps/celestia/render/markers</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Render coloured markers.</short>
      </locale>
    </schema>


    <!-- LABEL OPTIONS -->
    <schema>
      <key>/schemas/apps/celestia/labels/star</key>
      <applyto>/apps/celestia/labels/star</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Label stars.</short>
      </locale>
