<gconfschemafile>
  <schemalist>
    <!-- MAIN OPTIONS -->
    <schema>
      <key>/schemas/apps/celestia/winWidth</key>
      <applyto>/apps/celestia/winWidth</applyto>
      <owner>celestia</owner>
      <type>int</type>
      <default>640</default>
      <locale name="C">
        <short>Render area width.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/winHeight</key>
      <applyto>/apps/celestia/winHeight</applyto>
      <owner>celestia</owner>
      <type>int</type>
      <default>480</default>
      <locale name="C">
        <short>Render area height.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/winX</key>
      <applyto>/apps/celestia/winX</applyto>
      <owner>celestia</owner>
      <type>int</type>
      <default>-1</default>
      <locale name="C">
        <short>Horizontal window position.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/winY</key>
      <applyto>/apps/celestia/winY</applyto>
      <owner>celestia</owner>
      <type>int</type>
      <default>-1</default>
      <locale name="C">
        <short>Vertical window position.</short>
      </locale>
    </schema>


    <!-- VISUAL OPTIONS -->
    <schema>
      <key>/schemas/apps/celestia/ambientLight</key>
      <applyto>/apps/celestia/ambientLight</applyto>
      <owner>celestia</owner>
      <type>float</type>
      <default>0.1</default>
      <locale name="C">
        <short>Ambient light value (default: low).</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/visualMagnitude</key>
      <applyto>/apps/celestia/visualMagnitude</applyto>
      <owner>celestia</owner>
      <type>float</type>
      <default>8.5</default>
      <locale name="C">
        <short>Visual magnitude.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/showLocalTime</key>
      <applyto>/apps/celestia/showLocalTime</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Display time in terms of local time zone.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/hudDetail</key>
      <applyto>/apps/celestia/hudDetail</applyto>
      <owner>celestia</owner>
      <type>int</type>
      <default>1</default>
      <locale name="C">
        <short>Verbosity of information text displayed.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/fullScreen</key>
      <applyto>/apps/celestia/fullScreen</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Fullscreen state.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/starStyle</key>
      <applyto>/apps/celestia/starStyle</applyto>
      <owner>celestia</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
        <short>Style of star rendering.</short>
		<long>0=Fuzzy Points, 1=Points, 2=Scaled Discs</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/altSurfaceName</key>
      <applyto>/apps/celestia/altSurfaceName</applyto>
      <owner>celestia</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
        <short>Alternate surface description.</short>
      </locale>
    </schema>


    <!-- RENDER OPTIONS -->
    <schema>
      <key>/schemas/apps/celestia/render/stars</key>
      <applyto>/apps/celestia/render/stars</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Render stars.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/planets</key>
      <applyto>/apps/celestia/render/planets</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Render planets.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/galaxies</key>
