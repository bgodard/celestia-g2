        <short>Render constellation lines.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/cloudMaps</key>
      <applyto>/apps/celestia/render/cloudMaps</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Render clouds on planetary bodies.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/orbits</key>
      <applyto>/apps/celestia/render/orbits</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Render orbit lines.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/celestialSphere</key>
      <applyto>/apps/celestia/render/celestialSphere</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Render celestial grid lines.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/nightMaps</key>
      <applyto>/apps/celestia/render/nightMaps</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Render night-side textures on planets.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/atmospheres</key>
      <applyto>/apps/celestia/render/atmospheres</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Render atmosphere on planets.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/smoothLines</key>
      <applyto>/apps/celestia/render/smoothLines</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Render lines anti-aliased.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/eclipseShadows</key>
      <applyto>/apps/celestia/render/eclipseShadows</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Render shadows of bodies on other bodies.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/starsAsPoints</key>
      <applyto>/apps/celestia/render/starsAsPoints</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Render stars as single-pixel points.</short>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/celestia/render/ringShadows</key>
      <applyto>/apps/celestia/render/ringShadows</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short>Render shadows cast by planetary rings.</short>
      </locale>
