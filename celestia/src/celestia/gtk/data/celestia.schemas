    <schema>
      <key>/schemas/apps/celestia/labels/comet</key>
      <applyto>/apps/celestia/labels/comet</applyto>
      <owner>celestia</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
        <short>Label comet objects.</short>
      </locale>
    </schema>


    <!-- ORBIT VIEW OPTIONS -->
